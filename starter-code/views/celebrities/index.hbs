<h2>Celebrities</h2>
<ul>
    {{#each allCelebs}}
    <li>{{name}}</li>
    <a href="/celebrities/{{_id}}">Celebrity details</a>
    <form action="/celebtities/update/{{_id}}" method="post">
        <button type="submit">Update</button>
    </form>
    <form action="/celebtities/delete/{{_id}}" method="post">
        <button type="submit">Delete</button>
    </form>
    {{/each }}
</ul>